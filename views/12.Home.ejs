                        <label for="Semester">Select Semester:</label>
                        <select name="Semester" id="Semester" class="DataSel">
                        <option selected disabled>Select</option>    
                        <option value="sem1">1st Semester</option>
                        <option value="sem2">2nd Semester</option>
                        <option value="sem3">3rd Semester</option>
                        <option value="sem4">4th Semester</option>
                        <option value="sem5">5th Semester</option>
                        <option value="sem6">6th Semester</option>
                        <option value="sem7">7th Semester</option>
                        <option value="sem8">8th Semester</option>
                        </select>
                        <br><br>
                        <label for="Branch">Select Branch:</label>
                        <select name="Branch" id="Branch" class="DataSel">
                        <option selected disabled>Select</option>    
                        <option value="cse">CSE</option>
                        <option value="ise">ISE</option>
                        <option value="me">ME</option>
                        <option value="cve">CVE</option>
                        <option value="eee">EEE</option>
                        <option value="ece">ECE</option>
                        </select>
                        <br><br>
                        <label for="Doc">Select Document:</label>
                        <select name="Doc" id="Doc" class="DataSel">
                        <option selected disabled>Select</option>    
                        <option value="Micro-Assessment.pdf">Micro Assessment</option>
                        <option value="CO-PO Mapping.pdf">CO-PO Mapping</option>
                        <option value="CO Feedback.pdf">CO Feedback</option>
                        <option value="NBA Feedback.pdf">NBA Feedback</option>
                        </select>
                        <br><br>
                        <input type="submit" value="Submit" class="sub">
                        <input type="reset" value="Reset" class="res">
